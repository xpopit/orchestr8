{
  "name": "orchestr8",
  "version": "5.5.0",
  "description": "Enterprise-grade hierarchical multi-agent orchestration with ultra-fast Rust MCP stdio server (74 agents, <1ms queries, zero port conflicts, project-scoped, precompiled binaries)",
  "author": {
    "name": "Seth Schultz",
    "email": "orchestr8@sethschultz.com"
  },
  "license": "MIT",
  "repository": "https://github.com/seth-schultz/orchestr8",
  "keywords": [
    "orchestration",
    "multi-agent",
    "autonomous",
    "enterprise",
    "rust",
    "mcp",
    "stdio",
    "agent-discovery",
    "context-optimization",
    "high-performance"
  ],
  "features": {
    "agents": 74,
    "workflows": 20,
    "skills": 4,
    "plugins": 18,
    "hierarchical": true,
    "autonomous": true,
    "modular": true,
    "mcp_transport": "stdio",
    "database": "duckdb-inmemory",
    "query_latency": "<1ms",
    "project_scoped": true,
    "token_reduction": "50%+"
  },
  "hooks": "${CLAUDE_PLUGIN_ROOT}/orchestr8-bin/hooks.json",
  "requirements": {
    "claude": ">=1.0.0"
  }
}
